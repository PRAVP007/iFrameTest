<div style="height:200px; width:100%;">
    <h3>I am Page 3</h3>
    <button id="button3">Click to Load Page 4</button>
    <iframe id="page3frame" onload="resizeFrame" src="/home/page4"></iframe>
    </div>
    <script>
        $("#button3").click(function (e) {
            $("#page3frame").attr("src", "/home/page4");
        });
        function resizeFrame(obj) {
            obj.style.height = obj.contentWindow.document.documentElement.scrollHeight + 'px';
        }
    </script>
